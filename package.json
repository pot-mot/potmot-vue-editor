{
  "name": "potmot-vue-editor",
  "private": false,
  "version": "0.16.4",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "potmot"
  },
  "description": "a markdown editor for Vue3.",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/run-around---whats-wrong/potmot-vue-editor.git",
    "directory": "dist"
  },
  "bugs": "https://gitee.com/run-around---whats-wrong/potmot-vue-editor/issues",
  "main": "./dist/potmot-vue-editor.umd.cjs",
  "module": "./dist/potmot-vue-editor.js",
  "types": "./dist/components/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/potmot-vue-editor.js",
      "require": "./dist/potmot-vue-editor.umd.cjs"
    },
    "./*": "./*"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist",
    "src/assets",
    "src/components",
    "src/core",
    "src/declare",
    "src/directives",
    "src/hooks",
    "src/utils"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "dev": "vite development",
    "build": "vue-tsc --noEmit && vite build production",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/katex": "^0.16.2",
    "@types/markdown-it-container": "^2.0.6",
    "@vueuse/core": "^10.2.1",
    "d3": "^4.13.0",
    "file-saver": "^2.0.5",
    "katex": "^0.16.8",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-attributes": "^1.1.1",
    "markdown-it-container": "^3.0.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-multimd-table": "^4.2.2",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "mermaid": "^10.3.0",
    "v-viewer": "^3.0.11",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/lodash": "^4.14.195",
    "@types/markdown-it": "^12.2.3",
    "@types/prismjs": "^1.26.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "sass": "^1.32.7",
    "typescript": "^4.6.4",
    "vite": "^3.2.7",
    "vite-plugin-dts": "^2.1.0",
    "vite-plugin-prismjs": "^0.0.8",
    "vue-tsc": "^0.40.4"
