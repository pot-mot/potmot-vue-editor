{
  "name": "potmot-vue-editor",
  "private": false,
  "version": "0.17.0",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "potmot"
  },
  "description": "a markdown editor for Vue3.",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/run-around---whats-wrong/potmot-vue-editor.git",
    "directory": "dist"
  },
  "bugs": "https://gitee.com/run-around---whats-wrong/potmot-vue-editor/issues",
  "main": "./dist/potmot-vue-editor.umd.cjs",
  "module": "./dist/potmot-vue-editor.js",
  "types": "./dist/components/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/potmot-vue-editor.js",
      "require": "./dist/potmot-vue-editor.umd.cjs"
    },
    "./*": "./*"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist",
    "src/assets",
    "src/components",
    "src/core",
    "src/declare",
    "src/directives",
    "src/hooks",
    "src/utils"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/markdown-it": "^14.1.2",
    "@vueuse/core": "^10.11.1",
    "file-saver": "^2.0.5",
    "katex": "^0.16.22",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-attributes": "^1.2.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-multimd-table": "^4.2.3",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "mermaid": "^10.9.3",
    "prismjs": "^1.30.0",
    "v-viewer": "^3.0.22",
    "viewerjs": "^1.11.7",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/katex": "^0.16.7",
    "@types/lodash": "^4.17.20",
    "@types/markdown-it-container": "^2.0.10",
    "@types/prismjs": "^1.26.5",
    "@vitejs/plugin-vue": "^5.2.4",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-prismjs": "^0.0.8",
    "vue-tsc": "^2.2.12"
  }
}
